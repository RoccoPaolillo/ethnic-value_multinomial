f <- function(p,M,x,n){
if (x  <= (n*p)){U <- (x/(n*p))}
else {U <- M + ((1-(x/n))*(1-M))/(1-p)}
# else {U <- (2-M) + (((M-1)*x)/(n*p))} M + ((1-(x/n))*(1-M))/(1-p)}
round(U,digits = 3)
# print(U)
}
